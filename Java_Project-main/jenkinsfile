pipeline {
  agent any
  stages {
    stage ('BUILD') {
      steps {
        echo "cloning java project from git"
        sh ''' 
		    git clone https://github.com/Chandu1710/java19.git
	   '''
        }
    }
   stage ('Clone') {
		agent any
		steps {
			echo "Build a binary"
        sh ''' 
			mvn clean package
	   '''
      }
    }
  }
}
